<Window x:Class="TelephoneCompanyApp.View.SearchByNumber"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TelephoneCompanyApp.View"
        mc:Ignorable="d"
        Title="Поиск по номеру" Height="120" Width="300">
    <Grid>
        <StackPanel Margin="10">
            <Label Content="Введите номер:"/>
            <TextBox Name="Name"/>
            <Button Content="Поиск" Margin="10,5,0,0"/>
        </StackPanel>
    </Grid>
</Window>
